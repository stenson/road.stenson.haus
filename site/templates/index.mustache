<script>
  L.mapbox.accessToken = 'pk.eyJ1Ijoicm9ic3RlbnNvbiIsImEiOiI2eGhrQUhzIn0.grW_DxaZPTOdl2xcmga2Ng';
  var geodata = {{{geo-data}}};
  {{#latest}}
  var map = L.mapbox.map("map", "robstenson.2f9441c9", {
    scrollWheelZoom: false
  }).setView([{{lat}}, {{lng}}], 6);
  L.mapbox.featureLayer(geodata).addTo(map);
